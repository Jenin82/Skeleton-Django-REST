{
  "version": 2,
  "builds": [
    {
      "src": "Skeleton_Django_REST/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10",
        "command": "python manage.py collectstatic --noinput && python manage.py migrate && python manage.py runserver"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Skeleton_Django_REST/wsgi.py"
    }
  ]
}